{
  "name": "@simolation/vue-hotkey",
  "version": "2.0.1",
  "repository": "https://github.com/Simolation/vue-hotkey.git",
  "author": "Simon Osterlehner <simon@osterlehner.de>",
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "main": "./dist/vue-hotkey.umd.js",
  "module": "./dist/vue-hotkey.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-hotkey.es.js",
      "require": "./dist/vue-hotkey.umd.js"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "start:dev": "pnpm vite",
    "build": "pnpm vite build && yarn vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "do:publish": "npm publish --access=public"
  },
  "dependencies": {
    "vue-demi": "^0.14.10"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/types": "^7.20.7",
    "@types/node": "^24.0.4",
    "@vitejs/plugin-vue": "^6.0.0",
    "typescript": "^5.7.2",
    "vite": "^7.0.0",
    "vue": "^3.3.4",
    "vue-tsc": "^2.2.0"
  }
}
